{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.xlsx": "default",
    "*.svg": "default"
  },
  "workbench.tree.indent": 30,
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "right"
  },
  "editor.renderControlCharacters": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "git.autofetch": true,
  "html.format.indentHandlebars": true,
  "editor.formatOnSave": true,
  "editor.hover.enabled": true,
  "explorer.compactFolders": false,
  "diffEditor.wordWrap": "on",
  "html.format.wrapAttributesIndentSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.tabSize": 4,
  "explorer.confirmDragAndDrop": true,
  "explorer.confirmDelete": false,
  "material-icon-theme.folders.color": "#FFDBAC",
  "material-icon-theme.activeIconPack": "react_redux",
  "editor.inlineSuggest.enabled": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.autoImportCompletions": true,
  "files.autoSave": "afterDelay",
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.completeFunctionParens": false,
  "python.analysis.addImport.exactMatchOnly": true,
  "codeium.detectProxy": false,
  "editor.inlineSuggest.showToolbar": "always",
  "grunt.autoDetect": "on",
  "jake.autoDetect": "on",
  "gulp.autoDetect": "on",
  "ruff.args": ["[\"--ine-length=110\"]"],
  "flake8.args": ["--max-line-length=120", "--ignore=E402,F841,F401,E302,E305"],
  "database-client.telemetry.usesOnlineServices": false,
  "window.zoomLevel": -1,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "OM Theme (Night Owl Italic)",
  "files.associations": {
    ".flaskenv": "bat"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[javascript, javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "editor.stickyScroll.enabled": false,
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[json, jsonc]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css, scss, sass]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
